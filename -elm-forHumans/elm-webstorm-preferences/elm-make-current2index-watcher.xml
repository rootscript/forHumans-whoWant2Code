<?xml version="1.0" encoding="UTF-8"?>
<TaskOptions>
  <TaskOptions>
    <option name="arguments" value="./node_modules/elm/binwrappers/elm make $FilePathRelativeToProjectRoot$ ./src/$FileName$ --output ./build/index.js" />
    <option name="checkSyntaxErrors" value="true" />
    <option name="description" />
    <option name="exitCodeBehavior" value="ALWAYS" />
    <option name="fileExtension" value="elm" />
    <option name="immediateSync" value="false" />
    <option name="name" value="Elm Make CURRENT .elm to index.js" />
    <option name="output" value="$ProjectFileDir$/build/index.js" />
    <option name="outputFilters">
      <array />
    </option>
    <option name="outputFromStdout" value="false" />
    <option name="program" value="/usr/local/bin/node" />
    <option name="scopeName" value="Project Files" />
    <option name="trackOnlyRoot" value="false" />
    <option name="workingDir" value="$ProjectFileDir$" />
    <envs />
  </TaskOptions>
</TaskOptions>

